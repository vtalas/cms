<script type="text/ng-template" id="tree_item_renderer.html">
	<li ui-dropable-html="sortable,pageslist" ng-model="item" class="dropableCtrl" >.</li>
	<li ui-dropable-html="sortable,pageslist" ng-show="$index % 3 == 0 && !$first" ng-model="item" class="dropableCtrl" >-</li>
	<li ui-draggable-html="sortable" 
		 draggable="true" ng-model="item" 
		 ng-class="item.prdel" class="draggableCtrl">
		<div menuitem="item" grid="data" ></div> 
	</li>
	<li ui-dropable-html="sortable,pageslist" ng-show="$last" ui-config={last:true} class="dropableCtrl"  ng-model="item"></li>
</script>
<script type="text/ng-template" id="tree_item_rendererdiv.html">
	<div ui-dropable-html="sortable,pageslist" ng-model="item" class="dropableCtrl span1" ></div>
	<div ui-draggable-html="sortable" 
		 draggable="true" ng-model="item" 
		 ng-class="item.prdel" class="draggableCtrl span11">
		<div menuitem="item" grid="data" ></div> 
	</div>
	<div ui-dropable-html="sortable,pageslist" ng-show="$last" ui-config={last:true} ng-model="item" class="dropableCtrl span1"></div>
</script>
<script type="text/ng-template" id="tree_item_rendereSortable.html">
	<div ui-sortable-html="sortableXXX" 
		 ng-model="item" 
		 ng-class="item.prdel" class="draggableCtrl sortableCtrl">
		<div menuitem="item" grid="data" ></div> 
	</div>
</script>
<style>
	.sortableCtrl {
		margin-top: 10px;
	}
</style>
<div class="row-fluid " >
	<div class=" menu2">
		<ul class="ggg span4">
			@*<ng-include  ng-repeat="item in data.Children" src="'tree_item_renderer.html'"  >

			</ng-include>*@
			<li  ng-repeat="item in data.Children" ng-include="'tree_item_rendereSortable.html'" class="span{{item.Width * 4 || 4 }}" >

			</li>


		</ul>
	</div>
</div>

