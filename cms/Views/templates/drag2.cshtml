	}
	
</style>

<script type="text/javascript">
	$(document).ready(function (event) {
		$(".dropable").on("dragover", function (e) {
			e.stopPropagation();
			e.preventDefault();
			console.log("dragover");
		});
		$(".dropable").on("drop", function (e) {
			e.stopPropagation();
			e.preventDefault();
			console.log("drop", $(this));
		});
	});
</script>




